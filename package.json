{
  "name": "vue-uuid",
  "version": "1.0.0",
  "description": "Add UUID to Vue instance.",
  "main": "dist/vue-uuid.common.js",
  "module": "dist/vue-uuid.es.js",
  "scripts": {
    "test": "standard && ava",
    "build": "bili ./index.js -o dist --format cjs --format es --format umd --compress umd",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/vue-uuid.git"
  },
  "keywords": [
    "vue",
    "vue-uuid",
    "uuid"
  ],
  "author": "Vitor Luiz Cavalcanti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/vue-uuid/issues"
  },
  "homepage": "https://github.com/VitorLuizC/vue-uuid#README",
  "devDependencies": {
    "ava": "^0.23.0",
    "bili": "^0.18.2",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "uuid": "^3.1.0"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  }
}
